SET Hose = 'Dog'
WHERE Autumn &lt; 50 AND Spring &lt; 40;

SELECT *
FROM ArrayDaLads
WHERE 1 = 1;

SELECT ArrayDaLads.PupilId, ArrayDaLads.Forename, ArrayDaLads.Surname, ArrayDaLads.Hose, ArrayDaLads.RegGroup, ArrayDaLads.DOB, ArrayDaLads.Autumn, ArrayDaLads.Spring, ArrayDaLads.UCAS, MIN(ArrayDaLads.Autumn,ArrayDaLads.Spring,ArrayDaLads.UCAS) as Lowest, MAX(ArrayDaLads.Autumn,ArrayDaLads.Spring,ArrayDaLads.UCAS) as Highest, (ArrayDaLads.Autumn + ArrayDaLads.Spring + ArrayDaLads.UCAS) / 3 as Average
FROM ArrayDaLads
WHERE 1 = 1
ORDER BY (ArrayDaLads.Autumn + ArrayDaLads.Spring + ArrayDaLads.UCAS) / 3 desc;</sql><current_tab id="0"/></tab_sql></sqlb_project>
